<form method="post" action="{{ path('axe_delete', {'id': axe.id}) }}" onsubmit="return confirm('Suppression de l'axe ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ axe.id) }}">
    <button class="btn btn-outline-warning">Supprimer</button>
</form>
